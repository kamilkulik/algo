   "source": [
    "# count 1s in a binary representation of a number\n",
    "\n",
    "def count_bits(x: int) -> int:\n",
    "    bit_num = 0\n",
    "    while x:\n",
    "        bit_num += x & 1\n",
    "        x >>= 1\n",
    "    return bit_num\n",
    "\n",
    "count_bits(14)"
   ]
  }
